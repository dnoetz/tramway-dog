<%= content_tag(:div, "", id: "about", data: {
    controller: "about"
}) %>

<% content_for :meta_tags do %>
  <%= tag(:meta, name: "title", content: @page_title) %>
  <%= tag(:meta, name: "description", content: @meta_description) %>
  <%= tag(:meta, name: "keywords", content: @meta_keywords) %>
<% end %>